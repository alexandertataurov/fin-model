# Ultra Optimized NPM Configuration for Netlify
audit=false
fund=false
progress=false
save-exact=true
package-lock=true
prefer-offline=true
maxsockets=100
timeout=30000
registry=https://registry.npmjs.org/
loglevel=error
update-notifier=false
cache=.npm-cache
shamefully-hoist=true
legacy-peer-deps=true
strict-ssl=false
fetch-retries=3
fetch-retry-mintimeout=1000
fetch-retry-maxtimeout=5000