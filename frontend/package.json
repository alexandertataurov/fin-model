    "react-router-dom": "^6.8.1",
    "recharts": "^2.15.2",
    "sonner": "^2.0.6",
    "styled-components": "6.1.19",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^3.4.17",
    "tailwindcss-animate": "^1.0.7",
    "vaul": "^1.1.2",
    "vite": "^4.3.2",
