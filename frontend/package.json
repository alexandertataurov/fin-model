    "eslint": "^8.57.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.3.4",
    "eslint-plugin-unused-imports": "3.2.0",
    "globby": "^14.0.2",
    "jest-axe": "10.0.0",
    "jsdom": "^22.1.0",
    "msw": "2.10.4",
    "msw-storybook-addon": "2.0.5",
    "storybook": "8.6.14",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.2",
    "typescript": "^5.0.2",
    "vitest": "^0.34.0"
  },
  "pnpm": {
    "overrides": {
      "react": "^18.2.0",
      "react-dom": "^18.2.0"
    },
    "peerDependencyRules": {
      "allowAny": [
        "@types/react",
        "@types/react-dom"
      ]
    },
    "ignoredBuiltDependencies": []
  },
  "msw": {
    "workerDirectory": [
      "public"
    ]
  }
}
